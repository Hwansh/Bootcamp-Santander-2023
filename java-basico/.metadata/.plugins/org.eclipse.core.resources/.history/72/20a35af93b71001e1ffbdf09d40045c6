package application;

public abstract class primeiroIphone{
	public abstract void tocar();
	public abstract void pausar();
	public abstract void selecionarMusica();
	
	public abstract void ligar();
	public abstract void atender();
	public void iniciarCorreioVoz() {
		System.out.println("Correio de voz..");
	}

	public void exibirPagina() {
		System.out.println("Exibindo página..");
	}
	public void adicionarNovaAba() {
		System.out.println("Nova aba disponível..");
	}
	public void atualizarPagina() {
		System.out.println("Atualizando página..");
	}
	
	
//	public abstract void reprodutorMusical();
//	public abstract void aparelhoTelefonico();
//	public abstract void navegadorInternet();
}
